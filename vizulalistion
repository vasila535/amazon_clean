def rating_review_clean(value):
    result = str(value)
    if 'no rating_review' in result:
        return np.nan
    #elif 'no' in result:
        #return np.nan
    number = result.split()[0]
    number = number.replace(',', '')
    number = int(number)
    return number
    

df['rating_review'] = df['rating_review'].apply(rating_review_clean)
avg_price = df['rating_review'].mean()
print(avg_price)
df['rating_review'] = df['rating_review'].replace('no', avg_price)
df['rating_review'] = df['rating_review'].fillna(avg_price)
