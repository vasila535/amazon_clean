def rating_clean(value):
    result = str(value)
    if 'no rating' in result:
        return np.nan
    return result
    
 
   
df['rating'] = df['rating'].apply(rating_clean)
avg_price = df['rating'].mode()[0]
print(avg_price)
df['rating'] = df['rating'].fillna(avg_price)
    
