def cpu_model_clean(value):
    result = str(value)
    if 'no cpu_model' in result:
        return np.nan
    return result
    
 
   

df['cpu_model'] = df['cpu_model'].apply(cpu_model_clean)
avg_price = df['cpu_model'].mode()[0]
print(avg_price)
df['cpu_model'] = df['cpu_model'].fillna(avg_price)
    
